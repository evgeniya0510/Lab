#include <stdio.h>
#include <stdint.h>
#include <malloc.h>
typedef unsigned int unit32_t;
unit32_t *mas_1;
int i = 0;
int k,n = 0,m;
int puz (void)
{
	printf("Vvedite razmer massiva: ");
	scanf_s("%i", &n);
	mas_1 = (unit32_t*) malloc(n*sizeof(unit32_t));

	for(i = 0; i < n; i++)
    {
		printf("mas_1[%d] = ", i);
		scanf_s("%i", &mas_1[i]);
    }

	for (m = 0 ;m < n; m++)
	{
		for(i = 0;i < n; i++)
		{
			if (mas_1[i] > mas_1[i+1])
			{
				k = mas_1[i];
				mas_1[i] = mas_1[i+1];
				mas_1[i+1] = k;
			}
		}
	}
	 for(i=0; i<n; i++)
	 {
		free(mas_1);
	 }
	return 0;
}
void print_mas_1(void)
{
	for (i = 0; i < n; i++)
	{
		printf("%i ", mas_1[i]);
		
	}
	printf("\n");
}



int main()
{
	print_mas_1();
	puz();
	print_mas_1();
	return 0;
}

